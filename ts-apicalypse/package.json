{
  "name": "ts-apicalypse",
  "type": "module",
  "version": "0.4.0",
  "description": "A Typescript client and request builder for Apicalypse",
  "author": "JoÃ«l Charles <joel.charles91@gmail.com>",
  "keywords": [
    "apicalypse"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/magne4000/ts-apicalypse.git"
  },
  "homepage": "https://github.com/magne4000/ts-apicalypse/tree/main/ts-apicalypse",
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup"
  },
  "dependencies": {
    "axios": "^1.4.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "ts-toolbelt": "^9.6.0",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "clean": true,
    "dts": true
  }
}
